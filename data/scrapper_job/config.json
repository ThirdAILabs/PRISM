{
    "jobs": [
      {
        "name": "csl_data",
        "module": "jobs.csl",
        "fetcher": "fetch_source",
        "processor": "process_source",
        "updater": "update_local_store",
        "config": {
          "csv_url": "https://data.trade.gov/downloadable_consolidated_screening_list/v1/consolidated.csv",
          "original_file": "data/csl_data/original.csv",
          "output_file": "data/csl_data/new_data.csv"
        }
      },
      {
        "name": "doj_pr",
        "module": "jobs.doj",
        "fetcher": "fetch_articles",
        "processor": "process_articles",
        "updater": "update_articles",
        "config": {
          "rss_url_template": "https://www.justice.gov/news/rss?type=press_release&m={}",
          "start_date": "2025-01-01",
          "output_file": "data/doj_pr/doj_articles_with_content_and_entities.json",
          "country_keywords": [    
                "china",
                "chinese",
                "russia",
                "russian",
                "iran",
                "iranian"
            ],
          "academic_keywords": [  
                "professor",
                "university"
            ]
        }
      },
      {
        "name": "flagger_data",
        "module": "jobs.flagger",
        "fetcher": "fetch_flagger_source",
        "processor": "process_flagger_source",
        "updater": "update_flagger_store",
        "depends_on": ["csl_data"],
        "config": {
          "input_csv": "data/csl_data/new_data.csv",
          "output_institutions": "data/flagger_data/institutions.json",
          "output_funders": "data/flagger_data/funders.json",
          "output_publishers": "data/flagger_data/publishers.json"
        }
      },
      {
        "name": "university_au",
        "module": "jobs.university",
        "fetcher": "crawl_university_data",
        "processor": "process_university_data",
        "updater": "update_university_store",
        "config": {
          "start_urls": ["https://unitracker.aspi.org.au/"],
          "intermediate_json": "data/university_au/unitracker_au_new.json",
          "final_store": "data/university_au/unitracker_au.json"
        }
      },
      {
        "name": "university_webpages",
        "module": "jobs.university_webpages",
        "fetcher": "crawl_university_webpages",
        "processor": "process_university_webpages",
        "updater": "update_university_webpages",
        "depends_on": ["university_au"],
        "config": {
          "input_json": "data/university_au/unitracker_au_new.json",
          "output_file": "data/university_webpages/university_webpages_new.json"
        }
      }
    ],
